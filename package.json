{
   "name": "graphql",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcrypt": "^3.0.0",
      "@types/express": "^4.17.9",
      "@types/ioredis": "^4.17.8",
      "@types/jest": "^26.0.16",
      "@types/node": "^14.14.10",
      "@types/node-fetch": "^2.5.7",
      "@types/uuid": "^8.3.0",
      "@types/yup": "^0.29.10",
      "jest": "^26.6.3",
      "nodemon": "^2.0.6",
      "start-server-and-test": "^1.11.6",
      "ts-jest": "^26.4.4",
      "ts-node": "9.1.0",
      "typescript": "^4.1.2",
      "uuid": "^8.3.1"
   },
   "dependencies": {
      "@gql2ts/from-schema": "^2.0.0-4",
      "bcrypt": "^5.0.0",
      "express": "^4.17.1",
      "gql2ts": "^1.10.1",
      "graphql": "^15.4.0",
      "graphql-import": "^1.0.2",
      "graphql-request": "^3.3.0",
      "graphql-tools": "^7.0.2",
      "graphql-yoga": "^1.18.3",
      "ioredis": "^4.19.2",
      "node-fetch": "^2.6.1",
      "pg": "^8.5.1",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.29",
      "yup": "^0.32.1"
   },
   "scripts": {
      "start": "SET NODE_ENV=dev&nodemon --exec ts-node src/index.ts",
      "test-server": "SET NODE_ENV=test&ts-node src/index.ts",
      "test": "SET NODE_ENV=test&jest --watch",
      "test-all": "start-server-and-test test-server http://localhost:4000 test",
      "gen-schema-types": "ts-node src/scripts/createTypes.ts"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
